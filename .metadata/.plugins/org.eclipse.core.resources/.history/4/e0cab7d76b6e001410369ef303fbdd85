	private Piece[][] lista;
	
	@Override
	public Piece getPiece(String id) {
		Iterator<Piece> it = lista.iterator();
		while(it.hasNext()) {
			Piece current = it.next();
			if(current.getId()==id)
				return current;
		}
		return null;
	}
	
